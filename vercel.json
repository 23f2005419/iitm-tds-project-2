{
    "version": 2,
    "builds": [
      {
        "src": "src/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.py"
      }
    ],
    "functions": {
      "src/app.py": {
        "runtime": "python@3.9"
      }
    }
  
  }